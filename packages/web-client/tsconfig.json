{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "jsx": "react",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "strict": true
  },
  "parserOptions": {},

  "include": ["./src/**/*", "./src/**.*.scss"],
  "exclude": ["./node_modules/**/*"]
}
