{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "jsx": "react",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true
  },
  "parserOptions": {

  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules/**/*"
  ]
}